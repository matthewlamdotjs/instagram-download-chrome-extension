{
  "manifest_version": 2,
  "name": "Instagram Media Downloader",
  "description": "Download Instagram Videos and Images",
  "version": "1.1",
  "author": "Matthew Lam",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ],
  "content_scripts": [
    {
      "matches": ["https://*.instagram.com/*"],
      "js": ["stories.js"]
    }
  ]
}