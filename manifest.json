{
  "manifest_version": 2,
  "name": "IG Downloader",
  "description": "Downloads videos and images from ig",
  "version": "1.0",
  "author": "Matthew Lam",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ],
  "content_scripts": [
    {
      "matches": ["https://*.instagram.com/*"],
      "js": ["stories.js"]
    }
  ]
}